@model Category
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/AdminArea/Views/Shared/_AdminLayout.cshtml";
}
<div class="container">
    <div class="row">
    <h2>Detail</h2>
    <div class="tab-content">
        <div class="tab-pane fade active in" id="home">
            <h4>@Model.Name</h4>
            <p>
                @Model.Description
            </p>
            <a asp-action="Index" as class="btn btn-dark" style="font-size:30px">Return Back </a>

        </div>
    </div>
</div>
</div>


